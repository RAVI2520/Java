
public class String_palindrome {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String s="abcdsdcba";
		boolean palindrome=true;
		int i=0,j=s.length()-1;
		while(i<j)
		{
			if(s.charAt(i)!=s.charAt(j))
				palindrome=false;			
			i++;
			j--;
		}
	System.out.println(palindrome?s+" is plaindrome":s+" is not palindrome");
		

	}

}



//second Approch

public class StringPalindrome {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String s="asdfggfdsa1";
		int i=0,j=s.length()-1;
		boolean palindrome=true;
		while(i<=j)
		{
			if(s.charAt(i)!=s.charAt(j))
				palindrome=false;
			i++;
			j--;
		}
		
	System.out.println(palindrome?"This string is palindrome":"This string is not palindrome");

	}

}

