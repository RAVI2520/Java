
public class Two_Dimensional {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[][] arr= {{1,2},{3,2}};
		int[][] arr1= {{7,1},{8,2}};
		
		
		for(int i=0;i<arr.length;i++)
		{
			for(int j=0;j<arr[0].length;j++)
			{
				System.out.print(arr[i][j]+arr1[i][j]);
				
			}
			System.out.println();
		}

		
		
	}

}
