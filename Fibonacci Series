
public class Fibanacci_series {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//0,1,1,2,3,5,8,13,21
		int no1=0,no2=1,sum=0,limit=100;
		System.out.println(no1+"\n"+no2);
		
		while(sum<=100)
		{
			sum=no1+no2;
			no1=no2;
			no2=sum;
			
			System.out.println(sum<=100?sum:" ");
			
		}

	}

}
