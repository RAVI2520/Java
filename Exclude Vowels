
public class ExcludeVowels {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String s="Hello I am Raviraj Laxman Karande",wvowels="";
		char c='\0';
	
		for(int i=0;i<s.length();i++)
		{
			c=s.charAt(i)>='A'&&s.charAt(i)<='Z'?((char)(s.charAt(i)+32)):s.charAt(i);
//			System.out.println(c);
			if(c!='a'&&c!='e'&&c!='i'&&c!='o'&&c!='u')
				wvowels+=c;
		}
		System.out.print(wvowels);

	}

}
